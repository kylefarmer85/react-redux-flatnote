essages":"59","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/NoteShow.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/Note.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/index.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/Nav.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/NotesContainer.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/NewNote.js",["60"],"import React, { Component } from 'react';\nimport { Button, Form, Header } from 'semantic-ui-react'\nimport { connect } from 'react-redux'\nimport { addNote } from '../actions/notes'\n\nconst noteImages = ['../images/blue.png', '../images/orange.jpg', '../images/green.jpg', '../images/pink.png', '../images/yellow.jpg']\n\nconst randomNoteColor = noteImages[Math.floor(Math.random() * noteImages.length)]\n\nclass NewNote extends Component {\n  constructor(){\n    super()\n    this.state = {\n      title: '',\n      content: ''\n    }\n  }\n\n\n  handleChange = (e) => {\n    this.setState({\n      [e.target.name]: e.target.value\n    })\n  }\n\n  handleSubmit = (e) => {\n    e.preventDefault()\n\n    const reqObj = {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json'\n      },\n      body: JSON.stringify({\n        title: this.state.title,\n        content: this.state.content,\n        user_id: this.props.user.id,\n      })\n    }\n\n    fetch(\"http://localhost:3000/api/v1/notes\", reqObj)\n    .then(resp => resp.json())\n    .then(newNote => {\n      this.props.addNote(newNote)\n      this.props.history.push(`/notes/${newNote.id}`)\n    })\n  }\n\n\n  render() {\n    return (\n    <Form className='new-note-form' onSubmit={this.handleSubmit}>\n      <Header as='h1'>New Note</Header>\n      <Form.Field label=\"Note Title\" control=\"input\" name=\"title\" value={this.state.title} onChange={this.handleChange}/>\n      <Form.Field label='Note Content' control=\"textarea\" name=\"content\" value={this.state.content} onChange={this.handleChange}/>\n      <Button primary type='submit'>Submit</Button>\n    </Form>\n    );\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    user: state.user.user\n  }\n}\n\nexport default connect(mapStateToProps, {addNote})(NewNote);\n",["61","62"],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/actions/user.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/Login.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/App.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/EditNote.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/actions/notes.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/reducers/userReducer.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/reducers/notesReducer.js",[],"/Users/kylefarmer/Flatiron/mod4/Flatnote mod 4 project/react-redux-flatnote/src/components/Signup.js",[],{"ruleId":"63","severity":1,"message":"64","line":8,"column":7,"nodeType":"65","messageId":"66","endLine":8,"endColumn":22},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},"no-unused-vars","'randomNoteColor' is assigned a value but never used.","Identifier","unusedVar","no-native-reassign",["71"],"no-negated-in-lhs",["72"],"no-global-assign","no-unsafe-negation"]